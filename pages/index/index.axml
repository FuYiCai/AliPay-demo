<swiper indicator-dots="{{true}}" autoplay="{{true}}" indicator-active-color="#ff6699">
   <block a:for='{{carousels}}'>
      <swiper-item>
        <image src="{{item.imageUrl}}" class="swiper-img"/>
      </swiper-item>
    </block>
</swiper>

<view class="feature">
  <view >
    <image  class="feature-ico" src="/resources/icon/smallIco/fang.png" />
    全场包邮
  </view>
  <view >
    <image class="feature-ico" src="/resources/icon/smallIco/flash.png" />
    先行赔付
  </view>
  <view>
    <image class="feature-ico" src="/resources/icon/smallIco/yuan.png" />
    7天无理由
  </view>
  <view>
    <image class="feature-ico" src="/resources/icon/smallIco/ling.png" />
    优先退款
  </view>
</view>
<!-- 推荐商品 -->
<view class="list-title">
  <view class="title-tag">
    <image  src="/resources/icon/smallIco/rec.png" class="comm-image" />
